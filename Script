local box = messagebox("do u want to load?", "Eich Hub",4)

if box == 6 then
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/GreenDeno/Venyx-UI-Library/main/source.lua"))()
local venyx = library.new("Venyx", 5013109572)

-- themes
local themes = {
    Background = Color3.fromRGB(24, 24, 24),
    Glow = Color3.fromRGB(0, 0, 0),
    Accent = Color3.fromRGB(10, 10, 10),
    LightContrast = Color3.fromRGB(20, 20, 20),
    DarkContrast = Color3.fromRGB(14, 14, 14),
    TextColor = Color3.fromRGB(255, 255, 255)
}

-- first page
local page = venyx:addPage("Main", 5012544693)
local section1 = page:addSection("Main")
local page2 = venyx:addPage("Player")
local section2 = page2:addSection("Player")

section1:addToggle("Autofarm", nil, function(value)
    spawn(function()
    getgenv().autofarm = value
    while true do
        if not getgenv().autofarm then break end
        local A_1 = 1
        local Event = game:GetService("ReplicatedStorage").CoinToPlayer
        Event:FireServer(A_1)
        task.wait()
    end
end)
end)

section1:addToggle("Auto sell", nil, function(value)
    spawn(function()
    getgenv().autosell = value
    while true do
        if not getgenv().autosell then break end
game:GetService("ReplicatedStorage").Remotes.Sell:FireServer()
task.wait()
        end
    end)
end)


section2:addSlider("Walkspeed", 16, 16, 500, function(value)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value
end)



section2:addSlider("JumpPower", 50, 50, 250, function(value)
game.Players.LocalPlayer.Character.Humanoid.JumpPower = value
end)





section1:addToggle("Auto Buy Cosmos", nil, function(value)
    spawn(function()
    getgenv().autobuyc = value
    while true do
        if not getgenv().autobuyc then break end
local args = {
    [1] = "Cosmos"
}

game:GetService("ReplicatedStorage").Remotes.BuyAllSkills:InvokeServer(unpack(args))
task.wait()
    end
end)
end)



section1:addToggle("Auto Buy Speed", nil, function(value)
    spawn(function()
    getgenv().autobuys = value
    while true do
        if not getgenv().autobuys then break end
local args = {
    [1] = "Speed"
}

game:GetService("ReplicatedStorage").Remotes.BuyAllSkills:InvokeServer(unpack(args))
task.wait()
    end
end)
end)



section1:addToggle("Auto Buy Thunder", nil, function(value)
    spawn(function()
    getgenv().autobuyt = value
    while true do
        if not getgenv().autobuyt then break end
local args = {
    [1] = "Thunder"
}

game:GetService("ReplicatedStorage").Remotes.BuyAllSkills:InvokeServer(unpack(args))
task.wait()
    end
end)
end) -- for https://www.roblox.com/games/5827078246/FNAF-EGG-OP-Coins-Hero-Simulator

end
